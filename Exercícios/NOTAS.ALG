Algoritmo "Notas"

Var
nota1,nota2,total : real


Inicio

escreva("Digite sua primeira nota: ")
leia(nota1)

escreva("Digite sua segunda nota: ")
leia(nota2)

total <- nota1 + nota2

se total >= 60 entao
   escreval("Você foi aprovado!")
senao
     escreval("Você foi reprovado.")
fimse


Fimalgoritmo